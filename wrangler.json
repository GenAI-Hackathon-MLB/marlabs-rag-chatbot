{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "worker-cron-hono",
  "main": "src/index.ts",
  "compatibility_date": "2025-01-29",
  "observability": {
    "enabled": true
  },
  "env": {
    "dev": {
      "name": "worker-cron-hono-dev",
      "assets": {
        "directory": "./public"
      },
      "vars": {
        "ENVIRONMENT": "dev",
        "EMBEDDING_MODEL": "@cf/baai/bge-base-en-v1.5",
        "LLM_MODEL": "llama-3.3-70b-versatile",
        "SUMMARIZATION_MODEL": "mixtral-8x7b-32768"
      },
      "ai": {
        "binding": "AI"
      },
      "d1_databases": [
        {
          "binding": "DB",
          "database_name": "test-chat-history",
          "database_id": "460db141-3663-452d-880b-652a3064644c"
        }
      ],
      "vectorize": [
        {
          "binding": "VECTORIZE",
          "index_name": "test-embeddings-index"
        }
      ],
      "triggers": {
        "crons": ["0 0 1 * *"]
      }
    },
    "staging": {
      "name": "mars-chatbot-staging",
      "assets": {
        "directory": "./public"
      },
      "vars": {
        "ENVIRONMENT": "staging",
        "EMBEDDING_MODEL": "@cf/baai/bge-base-en-v1.5",
        "LLM_MODEL": "llama-3.3-70b-versatile",
        "SUMMARIZATION_MODEL": "mixtral-8x7b-32768"
      },
      "ai": {
        "binding": "AI"
      },
      "d1_databases": [
        {
          "binding": "DB",
          "database_name": "test-chat-history",
          "database_id": "460db141-3663-452d-880b-652a3064644c"
        }
      ],
      "vectorize": [
        {
          "binding": "VECTORIZE",
          "index_name": "test-embeddings-index"
        }
      ],
      "triggers": {
        "crons": ["0 0 1 * *"]
      }
    },
    "production": {
      "name": "mars-chatbot-prod",
      "assets": {
        "directory": "./public"
      },
      "vars": {
        "ENVIRONMENT": "production",
        "EMBEDDING_MODEL": "@cf/baai/bge-base-en-v1.5",
        "LLM_MODEL": "llama-3.3-70b-versatile",
        "SUMMARIZATION_MODEL": "mixtral-8x7b-32768"
      },
      "ai": {
        "binding": "AI"
      },
      "d1_databases": [
        {
          "binding": "DB",
          "database_name": "test-chat-history",
          "database_id": "460db141-3663-452d-880b-652a3064644c"
        }
      ],
      "vectorize": [
        {
          "binding": "VECTORIZE",
          "index_name": "test-embeddings-index"
        }
      ],
      "triggers": {
        "crons": ["0 0 1 * *"]
      }
    }
  }
}
